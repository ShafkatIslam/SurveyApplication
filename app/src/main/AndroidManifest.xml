<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.professt.surveyapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/company_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SurveyActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SurveyActivity2" />
        <activity android:name=".SurveyActivity3" />
        <activity android:name=".SurveyActivity4" />
        <activity android:name=".SurveyActivity5" />
        <activity android:name=".SubmitActivity" />
        <activity android:name=".SurveyList.SurveyListActivity" />
        <activity android:name=".SurveyList.SurveyDetailsActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>